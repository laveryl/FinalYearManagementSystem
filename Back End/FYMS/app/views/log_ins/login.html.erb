<%= image_tag "tcd.jpg" %>
<h1>Log In</h1>
<div class= "Sign_Form">
  <h1>Log in</h1>
  <%= form_tag(:action => 'login_attempt') do %>
    <p>Username or Email:</br> <%= text_field_tag(:username_or_email) %></p> 
    <p>Password:</br> <%= password_field_tag :login_password %></p> 
    <%= submit_tag("Log In") %>
    <%= link_to( "Create New Log In", {
      :controller => 'admin_log_ins', 
      :action => 'new'})%>
</div>
  <% end %>
</div>